s，第三方插件js，json文件，图片文件等等）\r\nimport { getSearchResults } from '@/api/search'\r\nexport default {\r\n  name: 'SearchResult',\r\n  components: {},\r\n  // 定义属性\r\n  props: {\r\n    searchText: {\r\n      type: String,\r\n      require: true\r\n    }\r\n  },\r\n  data () {\r\n    return {\r\n      list: [],\r\n      loading: false,\r\n      finished: false,\r\n      page: 1,\r\n      perpage: 10,\r\n      error: false\r\n    }\r\n  },\r\n  // 计算属性，会监听依赖属性值随之变化\r\n  computed: {},\r\n  // 监控data中的数据变化\r\n  watch: {},\r\n  // 方法集合\r\n  methods: {\r\n    async onLoad () {\r\n      // 异步更新数据\r\n      // setTimeout 仅做示例，真实场景中一般为 ajax 请求\r\n      if (this.searchText.length > 0) {\r\n        try {\r\n          const { data } = await getSearchResults({ page: this.page, per_page: this.perpage, q: this.searchText })\r\n          const { results } = data.data\r\n          this.list.push(...results)\r\n          this.loading = false\r\n          if (results.length) {\r\n            this.page++\r\n          } else {\r\n            this.finished = true\r\n          }\r\n        } catch (error) {\r\n          this.error = true\r\n          this.loading = false\r\n          this.$toast('获取搜索结果失败！')\r\n        }\r\n      }\r\n    }\r\n  },\r\n  // 生命周期 - 创建完成（可以访问当前this实例）\r\n  created () {\r\n  },\r\n  // 生命周期 - 挂载完成（可以访问DOM元素）\r\n  mounted () {},\r\n  beforeCreate () {}, // 生命周期 - 创建之前\r\n  beforeMount () {}, // 生命周期 - 挂载之前\r\n  beforeUpdate () {}, // 生命周期 - 更新之前\r\n  updated () {}, // 生命周期 - 更新之后\r\n  beforeDestroy () {}, // 生命周期 - 销毁之前\r\n  destroyed () {}, // 生命周期 - 销毁完成\r\n  activated () {} // 如果页面有keep-alive缓存功能，这个函数会触发\r\n}\r\n</script>\r\n\r\n<style lang='less' scoped>\r\n.search-result {\r\n  margin-top:104px;\r\n}\r\n</style>\r\n"]}]}