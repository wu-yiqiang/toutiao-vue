;AAXA;AAAA,uBAYA,KAAA,CAAA,IAAA,CAZA;;AAAA;AAAA;AAYA,gBAAA,IAZA,gBAYA,IAZA;AAaA,gBAAA,OAAA,CAAA,GAAA,CAAA,IAAA;AAbA;AAAA;AAAA,uBAeA,KAAA,CAAA,IAAA,CAfA;;AAAA;AAAA;AAeA,gBAAA,KAfA,iBAeA,IAfA;AAgBA;AACA,gBAAA,OAAA,CAAA,GAAA,CAAA,KAAA;;AACA,gBAAA,KAAA,CAAA,MAAA,CAAA,MAAA,CAAA,SAAA,EAAA,KAAA,CAAA,IAAA;;AACA,gBAAA,KAAA,CAAA,MAAA,CAAA,OAAA,CAAA,OAAA;;AACA,gBAAA,KAAA,CAAA,OAAA,CAAA,IAAA;;AApBA;AAAA;;AAAA;AAAA;AAAA;;AAsBA,gBAAA,KAAA,CAAA,MAAA,CAAA,IAAA,CAAA,QAAA,EAAA,YAAA,OAAA;;AACA,oBAAA,YAAA,QAAA,CAAA,MAAA,KAAA,GAAA,EAAA;AACA,kBAAA,KAAA,CAAA,MAAA,CAAA,IAAA,CAAA,YAAA;AACA,iBAFA,MAEA;AACA,kBAAA,KAAA,CAAA,MAAA,CAAA,IAAA,CAAA,eAAA;AACA;;AA3BA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AA6BA,KA9BA;AAgCA,IAAA,SAhCA,uBAgCA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAIA,MAAA,CAAA,KAAA,CAAA,SAAA,CAAA,QAAA,CAAA,QAAA,CAJA;;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA,kDAMA,OAAA,CAAA,GAAA,CAAA,MAAA,eANA;;AAAA;AAQA;AACA,gBAAA,MAAA,CAAA,eAAA,GAAA,IAAA,CATA,CAUA;;AAVA;AAAA;AAAA,uBAYA,OAAA,CAAA,MAAA,CAAA,IAAA,CAAA,MAAA,CAZA;;AAAA;AAYA,gBAAA,GAZA;AAaA,gBAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AAbA;AAAA;;AAAA;AAAA;AAAA;;AAeA,oBAAA,aAAA,QAAA,CAAA,MAAA,KAAA,GAAA,EAAA;AACA,kBAAA,MAAA,CAAA,MAAA,CAAA,gBAAA;AACA,iBAFA,MAEA;AACA,kBAAA,MAAA,CAAA,MAAA,CAAA,aAAA;AACA;;AAnBA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAqBA;AArDA;AAnBA,CAAA","sourcesContent":["<template>\r\n  <div class=\"login-container\">\r\n    <!-- 导航栏 -->\r\n    <van-nav-bar title=\"登录\" left-arrow>\r\n      <van-icon name=\"close\"  slot=\"left\" @click=\"$router.back()\"/>\r\n    </van-nav-bar>\r\n    <!-- 登录表单 -->\r\n    <van-form  ref=\"loginForm\" @submit=\"onSubmit\">\r\n      <van-field v-model=\"user.mobile\" name=\"mobile\"  placeholder=\"请输入手机号\" :rules=\"userFormRules.mobile\" type=\"number\" maxlength=\"11\">\r\n          <i slot=\"left-icon\" class=\"iconfont iconshouji\"></i>\r\n      </van-field>\r\n      <van-field  v-model=\"user.code\" name=\"code\" placeholder=\"请输入验证码\" :rules=\"userFormRules.code\" type=\"number\" maxlength=\"6\">\r\n          <i slot=\"left-icon\" class=\"iconfont iconyanzhengma\"></i>\r\n          <template #button>\r\n            <van-count-down :time=\"time\"  format=\"ss s\" v-if=\"isCountDownShow\" @finsh=\"isCountDownShow=false\"/>\r\n            <van-button size=\"small\" round type=\"default\" class=\"send-sms-btn\" @click=\"onSendSms\" v-else>发送验证码</van-button>\r\n          </template>\r\n      </van-field>\r\n      <div class=\"login-btn-wrap\">\r\n        <van-button  block type=\"info\" class=\"login-btn\" native-type=\"submit\">登录</van-button>\r\n      </div>\r\n    </van-form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { login, sendSms } from '@/api/user'\r\nexport default {\r\n  name: 'LoginIndex',\r\n  data () {\r\n    return {\r\n      user: {\r\n        mobile: '13911111112',\r\n        code: '246810'\r\n      },\r\n      userFormRules: {\r\n        mobile: [{ required: true, message: '请填写手机号码' }, { pattern: /^1[3|5|7|8]\\d{9}$/, message: '手机号码格式错误' }],\r\n        code: [{ required: true, message: '请填写验证码' }, { pattern: /^\\d{6}$/, message: '验证码格式错误' }]\r\n      },\r\n\r\n      time: 1000 * 60, // 倒计时时长\r\n\r\n      isCountDownShow: false // 显示倒计时的显示与隐藏\r\n\r\n    }\r\n  },\r\n  methods: {\r\n    async onSubmit () {\r\n      /* 提交表单 */\r\n      // 1.获取表单数据\r\n      const user = this.user\r\n      // 2.表单验证数据\r\n      this.$toast.loading({\r\n        message: '登陆中...',\r\n        forbidClick: true,\r\n        Duration: 0\r\n      })\r\n\r\n      // 3.提交表单请求\r\n      const { data } = await login(user)\r\n      console.log(data)\r\n      try {\r\n        const { data } = await login(user)\r\n        // 获取用户token\r\n        console.log(data)\r\n        this.$store.commit('setUser', data.data)\r\n        this.$toast.success('登录成功！')\r\n        this.$router.back()\r\n      } catch (error) {\r\n        this.$toast.fail('登录出现错误', error.message)\r\n        if (error.response.status === 400) {\r\n          this.$toast.fail('手机号或验证码错误！')\r\n        } else {\r\n          this.$toast.fail('登陆出现错误，请稍后重试！')\r\n        }\r\n      }\r\n    },\r\n\r\n    async onSendSms () {\r\n      /* 发送验证码 */\r\n      // 1.校验手机号码的有效性\r\n      try {\r\n        await this.$refs.loginForm.validate('mobile')\r\n      } catch (error) {\r\n        return console.log('验证失败', error)\r\n      }\r\n      // 2.验证通过,显示倒计时\r\n      this.isCountDownShow = true\r\n      // 3.发送验证码\r\n      try {\r\n        const res = await sendSms(this.user.mobile)\r\n        console.log(res)\r\n      } catch (err) {\r\n        if (err.response.status === 429) {\r\n          this.$toast('频繁发送验证码，请稍后重试！')\r\n        } else {\r\n          this.$toast('发送失败，请稍后重试！')\r\n        }\r\n      }\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.login-container{\r\n  .iconfont{\r\n    font-size:37px;\r\n  }\r\n  .send-sms-btn{\r\n    background-color: gray;\r\n    width: 152px;\r\n    height: 46px;\r\n    font-size: 22px;\r\n    line-height: 46px;\r\n    color: #ededed;\r\n  }\r\n  .login-btn-wrap{\r\n    padding:53px 33px;\r\n    .login-btn{\r\n      background-color: blue;\r\n      border:none;\r\n    }\r\n  }\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/login"}]}