n  })\r\n}\r\n\r\nexport const updateUserPhoto = (data) => {\r\n  /* 编辑当前用户头像 */\r\n  return request({\r\n    method: 'PATCH',\r\n    url: '/app/v1_0/user/photo',\r\n    data\r\n  })\r\n}\r\n"]}]}